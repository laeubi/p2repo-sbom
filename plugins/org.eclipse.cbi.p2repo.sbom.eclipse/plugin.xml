<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <command
            categoryId="org.eclipse.ui.category.help"
            defaultHandler="org.eclipse.cbi.p2repo.sbom.eclipse.handlers.GenerateSBOMHandler"
            id="org.eclipse.cbi.p2repo.sbom.eclipse.generateSBOM"
            name="Generate SBOM">
      </command>
   </extension>
   
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:help?after=additions">
         <command
               commandId="org.eclipse.cbi.p2repo.sbom.eclipse.generateSBOM"
               label="Generate SBOM..."
               style="push">
         </command>
      </menuContribution>
   </extension>
   
   <extension
         point="org.eclipse.help.toc">
      <toc
            file="html/toc.xml"
            primary="true">
      </toc>
   </extension>
   
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="contexts.xml">
      </contexts>
   </extension>
   
</plugin>
